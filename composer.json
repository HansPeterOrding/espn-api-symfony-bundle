{
  "name": "hans-peter-ording/espn-api-symfony-bundle",
  "type": "symfony-bundle",
  "description": "Symfony bundle to import data from ESPN api to symfony",
  "license": "MIT",
  "require": {
    "php": ">=8.1.0",
    "hans-peter-ording/espn-api-client": "^0.9",
    "symfony/config": "^5.1|^6.0|^7.0",
    "symfony/console": "^5.1|^6.0|^7.0",
    "symfony/dependency-injection": "^5.1|^6.0|^7.0",
    "symfony/http-kernel": "^5.1|^6.0|^7.0",
    "symfony/orm-pack": "^2.0|^6.0|^7.0"
  },
  "require-dev": {
    "symfony/maker-bundle": "^1.0@dev",
    "symfony/phpunit-bridge": "^5.1|^6.0|^7.0"
  },
  "autoload": {
    "psr-4": {
      "HansPeterOrding\\EspnApiSymfonyBundle\\": "src/"
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": { "HansPeterOrding\\EspnApiSymfonyBundle\\Tests\\": "tests/" }
  },
  "minimum-stability": "dev",
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
